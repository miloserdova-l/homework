<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pane" prefHeight="620.0" prefWidth="920.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.Controller">
    <children>
      <ProgressIndicator fx:id="indicator" layoutX="678.0" layoutY="310.0" prefHeight="45.0" prefWidth="50.0" progress="0.0" />
      <ImageView fx:id="photoAfter" fitHeight="450.0" fitWidth="450.0" layoutX="469.0" layoutY="108.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="photoBefore" fitHeight="450.0" fitWidth="450.0" layoutY="108.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="filterLabel" layoutX="14.0" layoutY="69.0" prefHeight="16.0" prefWidth="103.0" text="Select filter" />
      <ComboBox fx:id="filter" layoutX="103.0" layoutY="64.0" prefHeight="26.0" prefWidth="219.0" />
        <Button fx:id="selectFile" layoutX="14.0" layoutY="32.0" onAction="#fileSelector" prefHeight="26.0" prefWidth="306.0" text="Select a photo to apply a filter to it" />
      <Button fx:id="start" layoutX="377.0" layoutY="51.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="164.0" text="Start" onMouseClicked="#startFilters" />
      <Button fx:id="cancel" layoutX="601.0" layoutY="51.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="164.0" text="Cancel" onMouseClicked="#cancel" />
    </children>
</AnchorPane>
